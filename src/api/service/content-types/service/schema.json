{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "service_name": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "image",
        "feed"
      ],
      "required": true
    },
    "pdf": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": false
    },
    "feed_forms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feed-form.feed-form",
      "mappedBy": "service"
    },
    "images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::image.image",
      "mappedBy": "service"
    }
  }
}
